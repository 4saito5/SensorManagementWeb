{
  "name": "sensor-management-web",
  "version": "0.0.1",
  "main": "index.tsx",
  "author": "Densan Service Corporation",
  "license": "MIT",
  "scripts": {
    "build-prod-client": "webpack --config ./webpack.config.prod.js",
    "build-prod-server": "go build -o ./server/iot-servd ./server/main.go",
    "build": "yarn run build-prod-client && yarn run build-prod-server",
    "build-dev-client": "webpack --config ./webpack.config.dev.js",
    "watch": "webpack -w  --config ./webpack.config.dev.js",
    "lite": "lite-server",
    "server": "go run ./server/main.go",
    "start": "concurrently \"yarn run lite\" \"yarn run watch\" \"go run ./server/main.go\"",
    "db-create": "sudo mysql -u root --default-character-set=utf8 < ./db/create_table.sql",
    "db-up": "node_modules/.bin/db-migrate up",
    "db-down": "node_modules/.bin/db-migrate down"
  },
  "dependencies": {
    "history": "^4.7.2",
    "material-ui": "^0.20.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-tap-event-plugin": "^3.0.2",
    "redux-form": "^7.2.0",
    "redux-form-website-template": "^0.0.97"
  },
  "devDependencies": {
    "@types/react": "^16.0.28",
    "@types/react-dom": "^16.0.3",
    "awesome-typescript-loader": "^3.4.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "db-migrate": "^0.10.2",
    "db-migrate-mysql": "^1.1.10",
    "extract-text-webpack-plugin": "^3.0.2",
    "lite-server": "^2.3.0",
    "node-sass": "^4.7.2",
    "redux": "^3.7.2",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "tslib": "^1.8.1",
    "typescript": "^2.6.2",
    "uglify-js": "^3.2.1",
    "webpack": "^3.10.0"
  }
}
