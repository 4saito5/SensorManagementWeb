    "react-tap-event-plugin": "^3.0.2",
    "redux-form": "^7.2.0",
    "redux-form-website-template": "^0.0.97"
  },
  "devDependencies": {
    "@types/react": "^16.0.26",
    "@types/react-dom": "^16.0.3",
    "concurrently": "^3.5.1",
    "db-migrate": "^0.10.2",
